component NoButton:
    signal in
    signal out

# JLCPN: C318884
# easyeda2kicad --full --lcsc_id=C318884 --output elec/lib/lib
component ButtonTS1187A from NoButton:
    in ~ pin 1
    in ~ pin 2
    out ~ pin 3
    out ~ pin 4
    footprint = "SW-SMD_4P-L5.1-W5.1-P3.70-LS6.5-TL-2"

# MPN: SKRPACE010
