# this is a comment, it should be safely ignored
import Resistor from "generics/resistors.ato"
import Capacitor from "generics/capacitors.ato"

module LowPass:
    signal in
    signal out
    signal gnd

    r = new Resistor
    c = new Capacitor

    r.footprint = "R0402"
    c.footprint = "R0402"

    in ~ r.p1
    r.p2 ~ c.p1
    r.p2 ~ out
    c.p2 ~ gnd
